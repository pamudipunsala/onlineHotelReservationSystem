<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
		 http://xmlns.jcp.org/xml/ns/javee/web-app_3_1.xsd"
		 version="3.1">
		 
	<servelet>
		<servlet-name>ManagerLogin</servlet-name>
		<servelet-class>com.manager.ManagerLoginServlet</servelet-class>
	</servelet>
	<servlet-mapping>
		<servlet-name>ManagerLogin</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
		 
	<servlet>
		<servlet-name>ManagerInsertDetails</servlet-name>
		<servlet-class>com.manager.ManagerInsertDtlServlet</servlet-class>
	</servlet>	 
	<servelet-mapping>
		<servlet-name>ManagerInsert</servlet-name>
		<url-pattern>/insertDtl</url-pattern>
	</servelet-mapping>
		 
	<servlet>
		<servlet-name>UpdateManagerDtl</servlet-name>
		<servlet-class>com.manager.UpdateManagerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateManagerDtl</servlet-name>
		<url-pattern>/updateDtl</url-pattern>
	</servlet-mapping>
		 
	<servlet>
		<servlet-name>DeleteManagerDtl</servlet-name>
		<servlet-class>com.manager.DeleteManagerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteManagerDtl</servlet-name>
		<url-pattern>/deleteDtl</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>MeetingReview</servlet-name>
		<servlet-class>com.meeting.MeetingServlet</servlet-class>
	</servlet>
	<servelet-mapping>
		<servlet-name>MeetingReview</servlet-name>
		<url-pattern>/view</url-pattern>
	</servelet-mapping>
	
	<servlet>
		<servlet-name>MeetingArrange</servlet-name>
		<servlet-class>com.meeting.MeetingInsertServlet</servlet-class>
	</servlet>
	<servelet-mapping>
		<servlet-name>MeetingArrange</servlet-name>
		<url-pattern>/arrange</url-pattern>
	</servelet-mapping>
	
	<servelet>
		<servlet-name>MeetingUpdate</servlet-name>
		<servelet-class>com.meeting.MeetingUpdateServlet</servelet-class>
	</servelet>
	<servelet-mapping>
		<servlet-name>MeetingUpdate</servlet-name>
		<url-pattern>/update</url-pattern>
	</servelet-mapping>
	
	<servelet>
		<servlet-name>DeleteMeeting</servlet-name>
		<servelet-class>com.meeting.DeleteMeetingServlet</servelet-class>
	</servelet>
	<servelet-mapping>
		<servlet-name>DeleteMeeting</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servelet-mapping>
</web-app>